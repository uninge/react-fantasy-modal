{
  "name": "react-better-modal",
  "version": "0.0.5",
  "main": "dist/index.js",
  "author": "uninge <uninge.sun@gmail.com>",
  "description": "Accessible modal dialog component for React",
  "scripts": {
    "start": "rollup -c -w",
    "build": "rollup -c",
    "svgo": "svgo -f src/assets --config=./.svgo.yml",
    "clean": "rimraf ./dist",
    "prestart": "npm run clean",
    "prebuild": "npm run lint && npm run clean && npm run svgo",
    "prepublish": "npm run build",
    "lint": "eslint --fix --ext .ts,.tsx src",
    "lint-staged": "lint-staged",
    "test": "echo \"Error: no test specified\" && exit 1",
    "npm-check": "npm-check -u"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/runtime-corejs3": "^7.12.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-url": "^6.0.0",
    "@svgr/rollup": "^5.5.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/parser": "^4.8.1",
    "autoprefixer": "^10.2.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "core-js": "^3.7.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "less": "^4.1.1",
    "lint-staged": "^10.5.1",
    "postcss": "^8.2.4",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.33.3",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-stylelint": "^1.0.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "svgo": "^1.3.2",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "echo \"\n九尺高台，起于垒土。感谢有你！\n\""
    }
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "npm run lint",
      "prettier -c --write",
      "prettier --check --end-of-line auto"
    ]
  },
  "tags": [
    "react",
    "modal",
    "dialog",
    "window"
  ],
  "keywords": [
    "react",
    "reat-maodl",
    "reat-dialog",
    "reat-window",
    "modal",
    "dialog",
    "window",
    "react-component"
  ],
  "files": [
    "dist",
    "README.md",
    "package.json",
    "package-lock.json",
    "LICENSE"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "license": "MIT",
  "dependencies": {
    "npm-check": "^5.9.2"
  }
}
